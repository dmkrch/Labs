@page "{id:int}"@model WEB_953504_KARPOVICH.Areas.Admin.Pages.EditModel@{    ViewData["Title"] = "Edit";}<h1>Edit</h1><h4>Notebook</h4><hr /><div class="row">    <div class="col-md-4">        <form method="post">            <div asp-validation-summary="ModelOnly" class="text-danger"></div>            <input type="hidden" asp-for="Notebook.NotebookId" />            <div class="form-group">                <label asp-for="Notebook.NotebookName" class="control-label"></label>                <input asp-for="Notebook.NotebookName" class="form-control" />                <span asp-validation-for="Notebook.NotebookName" class="text-danger"></span>            </div>            <div class="form-group">                <label asp-for="Notebook.Description" class="control-label"></label>                <input asp-for="Notebook.Description" class="form-control" />                <span asp-validation-for="Notebook.Description" class="text-danger"></span>            </div>            <div class="form-group">                <label asp-for="Notebook.Price" class="control-label"></label>                <input asp-for="Notebook.Price" class="form-control" />                <span asp-validation-for="Notebook.Price" class="text-danger"></span>            </div>            <div class="form-group">                <label asp-for="Notebook.Image" class="control-label"></label>                <input asp-for="Notebook.Image" class="form-control" />                <span asp-validation-for="Notebook.Image" class="text-danger"></span>            </div>            <div class="form-group">                <label asp-for="Notebook.BrandId" class="control-label"></label>                <select asp-for="Notebook.BrandId" class="form-control" asp-items="ViewBag.BrandId"></select>                <span asp-validation-for="Notebook.BrandId" class="text-danger"></span>            </div>            <div class="form-group">                <input type="submit" value="Save" class="btn btn-primary" />            </div>        </form>    </div></div><div>    <a asp-page="./Index">Back to List</a></div>@section Scripts {    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}}